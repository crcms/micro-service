{
  "name": "crcms/microservice",
  "description": "The CrCms Microservice Project.",
  "keywords": [
    "crcms",
    "microservice",
    "microservice-framework",
    "rpc",
    "laravel",
    "swoole"
  ],
  "type": "project",
  "require": {
    "php": "^7.2.4",
    "crcms/repository": "^2.1",
    "crcms/microservice-framework": "0.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "CrCms\\App\\": "app/",
      "CrCms\\Extensions\\": "extensions/",
      "CrCms\\Modules\\": "modules/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CrCms\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php bin/crcms initialize",
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "@php bin/crcms initialize",
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php bin/crcms initialize",
      "@php bin/crcms key:generate --ansi"
    ],
    "post-autoload-dump": [
      "CrCms\\Microservice\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php bin/crcms package:discover --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.4"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  }
}
