###########################################################
###################### APP ENV ######################
###########################################################

APP_NAME=CRCMS
APP_ENV=local
APP_KEY=
APP_DEBUG=true

LOG_CHANNEL=daily

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=homestead
DB_USERNAME=homestead
DB_PASSWORD=secret

CACHE_DRIVER=file
QUEUE_CONNECTION=sync

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# swoole
SWOOLE_USER=
SWOOLE_GROUP=

# microservice
SERVICE_SECRET_STATUS=true
SERVICE_SECRET_CIPHER=AES-256-CFB
SERVICE_SECRET=


###########################################################
###################### Docker ENV ######################
###########################################################

NETWORKS_DRIVER=bridge
LOCAL_CODE_PATH=./
LOCAL_DOCKER_PATH=./docker
CONTAINER_CODE_PATH=/var/www
CONTAINER_DOCKER_PATH=/var/www/docker
# the application run mode
APP_RUN_PUID=1000
APP_RUN_PGID=1000
APP_RUN_NAME=crcms
APP_RUN_GROUP=crcms
TIMEZONE=Asia/Shanghai

### workspace ################################################
WORKSPACE_INSTALL_SWOOLE=true
WORKSPACE_INSTALL_MONGODB=false
WORKSPACE_INSTALL_REDIS=false
WORKSPACE_SWOOLE_PORT=28080
# Open later
WORKSPACE_SSH_PORT=22

### nginx ################################################
NGINX_HTTP_PORT=80
NGINX_SSL_PORT=443

### php-fpm ################################################
PHP_FPM_INSTALL_MONGODB=false
PHP_FPM_INSTALL_REDIS=false
PHP_FPM_PORT=9000

### consul ################################################